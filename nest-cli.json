{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/aleva-api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/aleva-api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/aleva-api-gateway",
  "projects": {
    "aleva-api-gateway": {
      "type": "application",
      "root": "apps/aleva-api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/aleva-api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/aleva-api-gateway/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/microservice/auth",
      "entryFile": "main",
      "sourceRoot": "apps/microservice/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/microservice/auth/tsconfig.app.json"
      }
    },
    "address": {
      "type": "application",
      "root": "apps/microservice/address",
      "entryFile": "main",
      "sourceRoot": "apps/microservice/address/src",
      "compilerOptions": {
        "tsConfigPath": "apps/microservice/address/tsconfig.app.json"
      }
    },
    "forms": {
      "type": "application",
      "root": "apps/microservice/forms",
      "entryFile": "main",
      "sourceRoot": "apps/microservice/forms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/microservice/forms/tsconfig.app.json"
      }
    },
    "booking": {
      "type": "application",
      "root": "apps/microservice/booking",
      "entryFile": "main",
      "sourceRoot": "apps/microservice/booking/src",
      "compilerOptions": {
        "tsConfigPath": "apps/microservice/booking/tsconfig.app.json"
      }
    },
    "billing": {
      "type": "application",
      "root": "apps/microservice/billing",
      "entryFile": "main",
      "sourceRoot": "apps/microservice/billing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/microservice/billing/tsconfig.app.json"
      }
    },
    "contracts": {
      "type": "library",
      "root": "libs/contracts",
      "entryFile": "index",
      "sourceRoot": "libs/contracts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/contracts/tsconfig.lib.json"
      }
    }
  }
}